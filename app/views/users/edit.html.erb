<h2>Edit Your Profile</h2>

<%= form_for @user do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit "Submit" :class => "button" %>
<% end %>

<div class="back">
  <%= link_to 'Back', events_path %>
</div>
